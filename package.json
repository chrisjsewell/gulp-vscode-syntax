{
    "name": "gulp-lattice",
    "displayName": "gulp-lattice",
    "description": "a syntax highlighter for input files to General Utility Lattice Program (GULP)",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gulp",
            "aliases": ["GULP", "gulp"],
            "extensions": [".gin"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gulp",
            "scopeName": "source.gulp",
            "path": "./syntaxes/gulp.tmLanguage.json"
        }]
    }
}